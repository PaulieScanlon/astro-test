---
const { title } = Astro.props;
---

<html lang='en'>
  <head>
    <meta charset='utf-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1' />
  </head>
  <body>
    <nav>
      <a href={`${import.meta.env.BASE_URL}`}>Home</a>
      <a href={`${import.meta.env.BASE_URL}about`}>About</a>
    </nav>
    <h1>{title}</h1>
    <article>
      <slot />
      <!-- your content is injected here -->
    </article>
  </body>
</html>
